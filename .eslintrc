{
    "parser": "babel-eslint",
    "extends": ["airbnb", "airbnb/hooks"],
    "env": {
      "node": true,
      "es6": true,
      "browser": true,
      "jest": true
    },
    "rules": {
      "no-console": "off",
      "import/prefer-default-export": "off",
      "max-len": ["error", 120, 2],
      "react/jsx-props-no-spreading": "off"
    },
    "settings": {
      "import/resolver": "webpack"
    }
  }